# Dealer Portal Credentials
# Copy this file to .env and fill in your actual credentials
# NEVER share or commit the .env file!

PORTAL_URL=https://dealer.example.com/login
PORTAL_USERNAME=your_username_here
PORTAL_PASSWORD=your_password_here

# Inventory page URL (if different from login redirect)
INVENTORY_URL=https://dealer.example.com/inventory

# Sync settings
SYNC_INTERVAL_HOURS=24
HEADLESS_MODE=true

# Alert settings (optional)
ALERT_EMAIL=your-email@example.com
